{
  "name": "bee-garden",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "cleanup:guests": "node ./scripts/cleanup-guest-accounts.js",
    "build": "npx expo export -p web && cp public/manifest.json dist/manifest.json && cp public/sw.js dist/sw.js && node scripts/inject-pwa.js",
    "build:sh": "./build.sh",
    "build:android": "eas build --profile local --platform android",
    "build:ios": "eas build --profile local --platform ios",
    "build:prod": "eas build --profile production --platform all",
    "godot:export:all": "export GODOT_EDITOR=\"/Applications/Godot4.5.app/Contents/MacOS/Godot\" && ./export_godot_GodotTest.sh ios && ./export_godot_GodotTest2.sh ios && ./export_godot_GodotTest.sh android && ./export_godot_GodotTest2.sh android",
    "godot:export:ios": "export GODOT_EDITOR=\"/Applications/Godot4.5.app/Contents/MacOS/Godot\" && ./export_godot_GodotTest.sh ios && ./export_godot_GodotTest2.sh ios",
    "godot:export:android": "export GODOT_EDITOR=\"/Applications/Godot4.5.app/Contents/MacOS/Godot\" && ./export_godot_GodotTest.sh android && ./export_godot_GodotTest2.sh android",
    "docker": "docker-compose up",
    "docker:build": "docker-compose up --build",
    "docker:ios": "docker-compose exec app npx expo start --ios",
    "docker:android": "docker-compose exec app npx expo start --android"
  },
  "dependencies": {
    "@borndotcom/react-native-godot": "^1.0.1",
    "@expo/metro-runtime": "~6.1.2",
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "@shopify/react-native-skia": "2.2.12",
    "@supabase/supabase-js": "^2.58.0",
    "axios": "^1.12.2",
    "expo": "~54.0.23",
    "expo-constants": "~18.0.9",
    "expo-device": "^8.0.9",
    "expo-file-system": "^19.0.17",
    "expo-font": "~14.0.8",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.8",
    "expo-linear-gradient": "~15.0.7",
    "expo-linking": "~8.0.8",
    "expo-location": "~19.0.7",
    "expo-pwa": "^0.0.127",
    "expo-router": "~6.0.14",
    "expo-secure-store": "~15.0.7",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.7",
    "expo-web-browser": "~15.0.9",
    "expo-widget": "^3.0.3",
    "framer-motion": "^12.23.24",
    "lucide-react": "^0.552.0",
    "planck-js": "^1.3.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1",
    "setimmediate": "^1.0.5",
    "suncalc": "^1.9.0"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "@types/suncalc": "^1.9.2",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "typescript": "~5.9.2"
  },
  "private": true
}
