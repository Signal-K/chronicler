{
  "name": "bee-garden",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "cleanup:guests": "node ./scripts/cleanup-guest-accounts.js",
    "build": "npx expo export -p web && cp public/manifest.json dist/manifest.json && cp public/sw.js dist/sw.js && node scripts/inject-pwa.js",
    "build:sh": "./build.sh",
    "build:android": "eas build --profile local --platform android",
    "build:ios": "eas build --profile local --platform ios",
    "build:prod": "eas build --profile production --platform all",
    "docker": "docker-compose up",
    "docker:build": "docker-compose up --build",
    "docker:ios": "docker-compose exec app npx expo start --ios",
    "docker:android": "docker-compose exec app npx expo start --android"
  },
  "svgo:assets": "svgo -f assets/images -q",
  "svgoConfig": {
    "multipass": true,
    "plugins": [
      {
        "name": "preset-default"
      },
      {
        "name": "removeDimensions"
      },
      {
        "name": "convertColors",
        "params": {
          "currentColor": true
        }
      },
      {
        "name": "cleanupIDs"
      }
    ]
  },
  "dependencies": {
    "@expo/metro-runtime": "~6.1.2",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/native": "^7.1.8",
    "@supabase/supabase-js": "^2.58.0",
    "axios": "^1.13.2",
    "expo": "~54.0.23",
    "expo-constants": "~18.0.9",
    "expo-haptics": "^15.0.8",
    "expo-image": "~3.0.8",
    "expo-linear-gradient": "~15.0.7",
    "expo-linking": "~8.0.8",
    "expo-location": "~19.0.7",
    "expo-router": "~6.0.14",
    "expo-secure-store": "~15.0.7",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "^1.0.8",
    "expo-system-ui": "~6.0.7",
    "expo-web-browser": "~15.0.9",
    "framer-motion": "^12.23.26",
    "lucide-react": "^0.562.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.17",
    "@types/node": "^24.10.1",
    "@types/react": "~19.1.10",
    "@types/suncalc": "^1.9.2",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "knip": "^5.79.0",
    "postcss": "^8.5.6",
    "react-native-svg-transformer": "^1.0.0",
    "svgo": "^3.0.0",
    "tailwindcss": "^4.1.17",
    "tw-animate-css": "^1.4.0",
    "typescript": "~5.9.2"
  },
  "private": true
}
